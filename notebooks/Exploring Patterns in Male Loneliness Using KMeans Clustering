    "# Columns we're working with\n",
    "selected_cols = [\n",
    "    'q0001', 'q0002',\n",
    "    'q0007_0001', 'q0007_0002', 'q0007_0003',\n",
    "    'q0007_0004', 'q0007_0010', 'q0007_0011',\n",
    "    'q0008_0008', 'q0008_0009', 'q0008_0010'\n",
    "]\n",
    "\n",
    "# Create a clean copy to work with\n",
    "survey_selected = survey[selected_cols].copy()\n",
    "\n",
    "\n",
    "## Create our mapping dictionaries\n",
    "# Q1: In general, how masculine or “manly” do you feel?\n",
    "masculinity_map = {\n",
    "    \"Not at all masculine\": 0,\n",
    "    \"Not very masculine\": 1,\n",
    "    \"Somewhat masculine\": 2,\n",
    "    \"Very masculine\": 3\n",
    "}\n",
    "\n",
    "# Q2: How important is it to you that others see you as masculine?\n",
    "importance_map = {\n",
    "    \"Not at all important\": 0,\n",
    "    \"Not too important\": 1,\n",
    "    \"Somewhat important\": 2,\n",
    "    \"Very important\": 3\n",
    "}\n",
    "\n",
    "# Q7 Behavioural Questions\n",
    "behavioural_map = {\n",
    "    \"Never, and not open to it\": 0,\n",
    "    \"Never, but open to it\": 1,\n",
    "    \"Rarely\": 2,\n",
    "    \"Sometimes\": 3,\n",
    "    \"Often\": 4\n",
    "}\n",
    "q7_cols = [\n",
    "    'q0007_0001', 'q0007_0002', 'q0007_0003', 'q0007_0004',\n",
    "    'q0007_0010', 'q0007_0011'\n",
    "]\n",
    "\n",
    "# Q8 Worry Questions\n",
    "worry_map = {\n",
    "    \"Not selected\": 0,\n",
    "    \"Your mental health\": 1,\n",
    "    \"Your physical health\": 1,\n",
    "    \"Your finances, including your current or future income, assets, or debt\": 1\n",
    "}\n",
    "q8_cols = [\n",
    "    'q0008_0008', 'q0008_0009', 'q0008_0010'\n",
    "]\n",
    "\n",
    "# Implement our mapping\n",
    "survey_selected['q0001'] = survey_selected['q0001'].map(masculinity_map)\n",
    "survey_selected['q0002'] = survey_selected['q0002'].map(importance_map)\n",
    "for col in q7_cols:\n",
    "    survey_selected[col] = survey_selected[col].map(behavioural_map)\n",
    "for col in q8_cols:\n",
    "    survey_selected[col] = survey_selected[col].map(worry_map)"
   ]
